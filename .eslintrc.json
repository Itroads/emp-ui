{
  "extends": [
      "plugin:react/recommended"
  ],
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "parser": "babel-eslint",
  "plugins": ["react"],
  "rules": {
      "react/jsx-uses-react": "error",
      "react/jsx-uses-vars": ["error"]
  }
}